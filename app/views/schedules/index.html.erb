<p style="color: green"><%= notice %></p>

<h1>Schedules</h1>

<div id="schedules">
  <% @schedules.each do |schedule| %>
    <%= render schedule %>
    <p>
      <%= link_to "Show this schedule", schedule %>
    </p>
  <% end %>
</div>
<%= render  "form", schedule: @schedule %>
<div data-controller="calendar">
<div id="calendar">
</div>
</div>
<%= link_to "New schedule", new_schedule_path %>
